{
  "name": "DubPlus",
  "version": "0.1.7",
  "description": "Dub+ - A simple script/extension for Dubtrack.fm",
  "main": "dubplus.js",
  "scripts": {
    "build": "node ./tasks",
    "build-release": "node ./tasks --release",
    "watch": "node ./tasks watch",
    "bundle": "node ./tasks bundle",
    "minify": "node ./tasks minify",
    "min-release": "node ./tasks min --release",
    "sass": "node ./tasks sass",
    "ext": "node ./tasks ext",
    "ext-zip": "node ./tasks ext-zip",
    "ext-deploy": "node ./tasks ext-deploy",
    "rollup": "rollup --config tasks/rollup.config.js --watch",
    "test": "ENV=test mocha -c -t 5000 --reporter=spec tests/.setup.js tests/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DubPlus/DubPlus.git"
  },
  "author": "DubPlus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DubPlus/DubPlus/issues"
  },
  "homepage": "https://dub.plus",
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-babili": "0.0.11",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^4.1.2",
    "chalk": "^1.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-preact": "^0.1.0",
    "fs-extra": "^2.0.0",
    "git-repo-info": "^1.4.1",
    "mocha": "^5.0.5",
    "node-sass": "^4.5.0",
    "postcss": "^5.2.14",
    "preact-compat-enzyme": "^0.2.5",
    "preact-test-utils": "^0.1.3",
    "preprocess": "^3.1.0",
    "request": "^2.80.0",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.4.8",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "preact": "^8.2.5",
    "preact-portal": "^1.1.2"
  }
}
