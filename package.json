    "postcss": "^5.2.18",
    "preact-compat": "^3.18.4",
    "preact-render-to-string": "^3.8.2",
    "preact-test-utils": "^0.1.3",
    "preprocess": "^3.1.0",
    "react-test-renderer": "^16.6.0",
    "request": "^2.88.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
