{
  "name": "DubPlus",
  "version": "2.0.0",
  "description": "Dub+ - A simple script/extension for Dubtrack.fm",
  "main": "dubplus.js",
  "scripts": {
    "sass": "node ./tasks/sassbundle",
    "ext": "node tasks/rollup.config.js && node ./tasks/extensions",
    "ext-zip": "node ./tasks ext-zip",
    "ext-deploy": "node ./tasks ext-deploy",
    "test": "jest -i ./tests --config ./tests/jest.config.js",
    "build": "node tasks/rollup.config.js",
    "beta": "BUILD=beta node tasks/rollup.config.js",
    "release": "BUILD=release node tasks/rollup.config.js",
    "start": "WATCH=true node tasks/rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DubPlus/DubPlus.git"
  },
  "author": "DubPlus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DubPlus/DubPlus/issues"
  },
  "homepage": "https://dub.plus",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^6.7.3",
    "babel-jest": "^24.1.0",
    "chalk": "^1.1.3",
    "fake-indexeddb": "^2.0.4",
    "fs-extra": "^2.0.0",
    "git-repo-info": "^1.4.1",
    "jest": "^24.0.0",
    "node-sass": "^4.11.0",
    "postcss": "^5.2.18",
    "preprocess": "^3.1.0",
    "request": "^2.88.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "preact-portal": "^1.1.3"
  }
}
