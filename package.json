{
  "name": "dubplus",
  "version": "1.0.0",
  "type": "module",
  "description": "Dub+ - A simple script/extension for QueUp.net",
  "main": "dubplus.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp ./dist/dubplus.* . && rm -rf ./dist",
    "preview": "vite preview",
    "ext": "node ./tasks ext",
    "zip": "node ./tasks zip",
    "start:firefox": "npm run ext && web-ext run --source-dir ./extensions/Firefox/",
    "test": "playwright test",
    "test:ui": "playwright test --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DubPlus/DubPlus.git"
  },
  "author": "DubPlus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DubPlus/DubPlus/issues"
  },
  "homepage": "https://dub.plus",
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@playwright/test": "^1.45.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^20.14.10",
    "chalk": "^5.3.0",
    "eslint-plugin-svelte": "^2.46.0",
    "svelte": "^5.17.5",
    "vite": "^6.0.7",
    "web-ext": "^8.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
