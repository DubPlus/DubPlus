{
  "name": "DubPlus",
  "version": "0.3.4",
  "description": "Dub+ - A simple script/extension for Dubtrack.fm and QueUp.net",
  "main": "dubplus.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "node ./tasks",
    "build-release": "node ./tasks --release",
    "watch": "node ./tasks watch",
    "bundle": "node ./tasks bundle",
    "minify": "node ./tasks minify",
    "min-release": "node ./tasks min --release",
    "sass": "node ./tasks sass",
    "ext": "node ./tasks ext",
    "ext-zip": "node ./tasks ext-zip",
    "ext-deploy": "node ./tasks ext-deploy",
    "start:firefox": "npm run ext && web-ext run --source-dir ./extensions/Firefox/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DubPlus/DubPlus.git"
  },
  "author": "DubPlus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DubPlus/DubPlus/issues"
  },
  "homepage": "https://dub.plus",
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "git-repo-info": "^2.1.1",
    "postcss": "^8.4.39",
    "uglifyify": "^5.0.2",
    "watchify": "^4.0.0",
    "web-ext": "^8.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/core": "^7.24.7",
    "node-fetch": "^2.7.0",
    "sass": "^1.77.6"
  }
}
