{
  "name": "DubPlus",
  "version": "2.0.0",
  "description": "Dub+ - A simple script/extension for Dubtrack.fm",
  "main": "dubplus.js",
  "scripts": {
    "build": "node ./tasks",
    "build-release": "node ./tasks --release",
    "watch": "node ./tasks watch",
    "bundle": "node ./tasks bundle",
    "minify": "node ./tasks minify",
    "min-release": "node ./tasks min --release",
    "sass": "node ./tasks sass",
    "ext": "node ./tasks ext",
    "ext-zip": "node ./tasks ext-zip",
    "ext-deploy": "node ./tasks ext-deploy",
    "rollup": "rollup --config tasks/rollup.config.js",
    "test": "ENV=test jest -i ./tests --config ./tests/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DubPlus/DubPlus.git"
  },
  "author": "DubPlus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DubPlus/DubPlus/issues"
  },
  "homepage": "https://dub.plus",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^6.7.3",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chalk": "^1.1.3",
    "fake-indexeddb": "^2.0.4",
    "fs-extra": "^2.0.0",
    "git-repo-info": "^1.4.1",
    "i": "^0.3.6",
    "jsdom": "^11.12.0",
    "node-sass": "^4.11.0",
    "postcss": "^5.2.18",
    "preact-compat": "^3.18.4",
    "preact-render-to-string": "^3.8.2",
    "preact-test-utils": "^0.1.3",
    "preprocess": "^3.1.0",
    "react-test-renderer": "^16.7.0",
    "request": "^2.88.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.1",
    "sinon": "^4.5.0"
  },
  "dependencies": {
    "jest": "^22.4.4",
    "preact": "^8.4.2",
    "preact-portal": "^1.1.3",
    "preact-render-spy": "^1.3.0"
