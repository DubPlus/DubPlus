{
  "manifest_version": 2,
  "name": "DubPlus",
  "short_name": "Dub+",
  "author": "------",
  "description": "Dub+ - A Dubtrack.fm script/extension for added features and customizations",
  "homepage_url": "https://dub.plus/",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.dubtrack.fm/*"
      ],
      "js": [
        "scripts/jquery.js",
        "scripts/loader.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}