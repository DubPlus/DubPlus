{
  "manifest_version": 3,
  "version": "0.0.0",
  "name": "DubPlus",
  "short_name": "Dub+",
  "author": "Dub+ Devs",
  "description": "Dub+ - A Dubtrack.fm and QueUp.net script/extension for added features and customizations",
  "homepage_url": "https://dub.plus/",
  "action": {
    "default_icon": {
      "16": "extension/icons/18.png",
      "38": "extension/icons/38.png",
      "48": "extension/icons/48.png"
    }
  },
  "content_scripts": [
    {
      "js": ["dubplus.js"],
      "css": ["dubplus.css"],
      "matches": ["*://*.dubtrack.fm/join/*", "*://*.queup.net/join/*"],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ],
  "icons": {
    "16": "extension/icons/18.png",
    "19": "extension/icons/19.png",
    "38": "extension/icons/38.png",
    "48": "extension/icons/48.png",
    "96": "extension/icons/96.png",
    "128": "extension/icons/128.png"
  },
  "host_permissions": [
    "*://dubtrack.fm/*",
    "*://*.dubtrack.fm/*",
    "*://queup.net/*",
    "*://*.queup.net/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["dubplus.js", "dubplus.css", "dubplus.js.map"],
      "matches": [
        "*://dubtrack.fm/*",
        "*://*.dubtrack.fm/*",
        "*://queup.net/*",
        "*://*.queup.net/*"
      ]
    }
  ]
}
